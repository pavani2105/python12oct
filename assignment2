sno=int(input())
sname=input()
group=input()
s1=int(input())
s2=int(input())
s3=int(input())
tot=s1+s2+s3
avg=tot/3
if avg>=90:
    print('o grade')
elif avg>=80:
    print('a grade')
elif avg>=70:
    print('b grade')
elif avg>=60:
    print('c grade')
elif avg>=50:
    print('d grade')
else:
    print('pass')
